<?xml version="1.0" encoding="UTF-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <OutputPath>bin\$(Configuration)\</OutputPath>
    <PyRevitRootDir>$(MSBuildThisFileDirectory)..\</PyRevitRootDir>
    <PyRevitBinDir>$(PyRevitRootDir)bin\</PyRevitBinDir>
    <PyRevitDevDir>$(PyRevitRootDir)dev\</PyRevitDevDir>
    <PyRevitModulesDir>$(PyRevitDevDir)modules\</PyRevitModulesDir>
  </PropertyGroup>

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net48</TargetFramework>
    <AppendTargetFrameworkToOutputPath>False</AppendTargetFrameworkToOutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Debug'">
    <DefineConstants>$(DefineConstants);DEBUG;TRACE</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <DefineConstants>$(DefineConstants);RELEASE</DefineConstants>
  </PropertyGroup>

  <PropertyGroup>
    <Version>4.8.8</Version>
    <Copyright>Copyright Â© 2014-2021</Copyright>
    <Company>pyRevitLabs.io</Company>
  </PropertyGroup>
</Project>