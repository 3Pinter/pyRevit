    <!-- pyrevit cli properties -->
    <PropertyGroup>
        <ProductIdCode></ProductIdCode>
        <ProductUpgradeCode></ProductUpgradeCode>
    </PropertyGroup>

    <!-- wix variables (var.*) -->
