        <DefineConstants>$(DefineConstants);ProductIdCode=$(ProductIdCode)</DefineConstants>
        <DefineConstants>$(DefineConstants);ProductUpgradeCode=$(ProductUpgradeCode)</DefineConstants>
    </PropertyGroup>
</Project>